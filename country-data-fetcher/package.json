{
  "name": "country-data-fetcher",
  "version": "1.0.0",
  "description": "",
  "main": "build/app.js",
  "scripts": {
    "build": "tsc -b src/tsconfig.app.json",
    "serve": "ts-node-dev src/app.ts",
    "start": "tsc && node build/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "@types/jest": "27.5.0",
    "@types/moxios": "^0.4.9",
    "@types/node": "^14.17.00",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.23.0",
    "jest": "28.1.0",
    "jest-mock-proxy": "^3.0.0",
    "moxios": "^0.4.0"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.5.0",
    "@graphql-tools/mock": "^8.7.0",
    "@graphql-tools/utils": "^7.6.0",
    "axios": "^0.21.1",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "elastic-apm-node": "^3.26.0",
    "express": "^4.18.2",
    "graphql": "^15.5.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-scalars": "^1.9.0",
    "graphql-tag-ts": "^0.1.16",
    "graphql-tools": "^8.1.0",
    "graphql-type-json": "^0.3.1",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.1.0",
    "module-alias": "^2.2.2"
  },
  "_moduleAliases": {
    "@": "./src"
  },
  "jest": {
    "verbose": false,
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "**/test/unit/**/*.spec.ts",
      "**/test/integration/**/*.ispec.ts"
    ],
    "globals": {},
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/apollo/*.ts",
      "!src/app.ts",
      "!src/server.ts",
      "!src/services/redisCacheService.ts",
      "!src/util/config.ts",
      "!src/util/logger.ts",
      "!src/tools/GenerateJsonFromGql.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "lines": 80
      }
    }
  }
}
